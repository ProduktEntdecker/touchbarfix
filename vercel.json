{
  "framework": null,
  "headers": [
    {
      "source": "/App/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    },
    {
      "source": "/downloads/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/App/:path*",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/.git/:path*", 
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/.github/:path*",
      "destination": "/404.html", 
      "statusCode": 404
    },
    {
      "source": "/docs/:path*",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/scripts/:path*",
      "destination": "/404.html",
      "statusCode": 404
    },
    {
      "source": "/CLAUDE.md",
      "destination": "/404.html",
      "statusCode": 404
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}